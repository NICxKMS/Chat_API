{
  "name": "ai-chat-interface",
  "version": "1.8.5",
  "private": true,
  "dependencies": {
    "@primer/octicons-react": "^19.15.1",
    "clsx": "^2.1.1",
    "firebase": "^11.6.0",
    "firebase-tools": "^14.1.0",
    "katex": "^0.16.22",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^8.0.7",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-virtuoso": "^4.12.6",
    "react-window": "^1.8.9",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-emoji": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-parse": "^11.0.0",
    "serve": "^14.2.1",
    "unified": "^11.0.5",
    "web-vitals": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@craco/craco": "^7.1.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.21",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "compression-webpack-plugin": "^11.1.0",
    "core-js": "^3.41.0",
    "cross-env": "^7.0.3",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "daisyui": "^5.0.28",
    "eslint": "^8.48.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^5.0.1",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.5.3",
    "rimraf": "^5.0.1",
    "shadcn-ui": "^0.9.5",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^16.19.0",
    "stylelint-config-standard": "^38.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "scripts": {
    "start": "cross-env REACT_APP_ENV=development craco start",
    "start:prod": "serve -s build",
    "dev": "cross-env REACT_APP_ENV=development craco start",
    "dev:fast": "cross-env REACT_APP_ENV=development FAST_REFRESH=true craco start --no-lazy",
    "build": "cross-env REACT_APP_ENV=production GENERATE_SOURCEMAP=true craco build",
    "build:prod": "cross-env REACT_APP_ENV=production craco build",
    "build:analyze": "cross-env REACT_APP_ENV=production ANALYZE=true craco build",
    "test": "craco test",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:ci": "cross-env CI=true craco test",
    "test:watch": "craco test --watch",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "analyze": "cross-env REACT_APP_ENV=production GENERATE_SOURCEMAP=true craco build && source-map-explorer 'build/static/js/*.js'",
    "postbuild": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "clean": "rimraf build node_modules",
    "reinstall": "npm run clean && npm install",
    "prebuild": "rimraf build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/**/*.stories.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "*.less",
    "*.module.css",
    "*.module.scss",
    "*.module.sass",
    "*.module.less"
  ]
}
