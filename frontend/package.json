{
  "name": "ai-chat-interface",
  "version": "1.8.8",
  "private": true,
  "dependencies": {
    "@primer/octicons-react": "^19.15.1",
    "clsx": "^2.1.1",
    "firebase": "^11.6.0",
    "firebase-tools": "^14.1.0",
    "katex": "^0.16.22",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^8.0.7",
    "react-syntax-highlighter": "^15.5.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-virtuoso": "^4.12.6",
    "react-window": "^1.8.9",
    "rehype-katex": "^7.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-emoji": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-parse": "^11.0.0",
    "serve": "^14.2.1",
    "unified": "^11.0.5",
    "web-vitals": "^3.0.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@prerenderer/renderer-puppeteer": "^1.2.4",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^2.0.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "core-js": "^3.41.0",
    "cross-env": "^7.0.3",
    "daisyui": "^5.0.28",
    "eslint": "^8.48.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.5.3",
    "puppeteer": "^24.9.0",
    "rimraf": "^5.0.1",
    "serve-handler": "^6.1.6",
    "shadcn-ui": "^0.9.5",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^16.19.0",
    "stylelint-config-standard": "^38.0.0",
    "vite": "^6.3.5",
    "vite-plugin-html-prerender": "^0.2.0",
    "vite-plugin-prerender": "^1.0.8",
    "vitest": "^2.0.4"
  },
  "scripts": {
    "start": "vite",
    "start:prod": "serve -s dist -l 3001",
    "dev": "vite",
    "dev:fast": "vite",
    "prebuild": "rimraf dist",
    "build": "vite build",
    "postbuild": "node prerender.js",
    "clean": "rimraf dist node_modules",
    "reinstall": "npm run clean && npm install",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "analyze": "vite build && source-map-explorer \"dist/assets/*.js\" --html dist/report.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/**/*.stories.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "*.less",
    "*.module.css",
    "*.module.scss",
    "*.module.sass",
    "*.module.less"
  ],
  "reactSnap": {
    "include": [
      "/"
    ],
    "inlineCss": true
  }
}
