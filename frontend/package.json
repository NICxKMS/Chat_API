{
  "name": "ai-chat-interface",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@primer/octicons-react": "^19.15.1",
    "firebase": "^11.6.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^8.0.7",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.9",
    "remark-gfm": "^4.0.1",
    "web-vitals": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "rimraf": "^5.0.1",
    "source-map-explorer": "^2.5.3"
  },
  "scripts": {
    "start": "cross-env REACT_APP_ENV=development react-scripts start",
    "dev": "cross-env REACT_APP_ENV=development react-scripts start",
    "build": "react-scripts build",
    "build:prod": "cross-env REACT_APP_ENV=production react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "cross-env CI=true react-scripts test",
    "test:watch": "react-scripts test --watch",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "clean": "rimraf build node_modules",
    "reinstall": "npm run clean && npm install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/**/*.stories.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
